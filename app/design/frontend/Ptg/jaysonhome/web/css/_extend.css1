url('fonts/proximanova-light-webfont.woff') format('woff'),
url('fonts/proximanova-light-webfont.ttf') format('truetype'),
url('fonts/proximanova-light-webfont.svg#proximanova-light-webfont') format('svg');
font-weight: normal;
font-style: normal;
		}
a {list-style:none; text-decoration:none;}
* {margin:0px; padding:0px;}


.header-top {    text-align: center;
    text-transform: none;
    font-size: 13px;
    padding: 7px 0 7px;
    letter-spacing: 0.1em;
    background: #000000;
    color: #fff;
    font-family: 'ProximaNova-Light';}
	
.header-top .container a.btn-top {font-family:"Myriad Pro"; font-size:13px; line-height:normal; color:#fff; padding:0px; margin:0px 0px 0px; font-weight:bold; font-style:italic;}
.header-top .container a.btn-top:after {content:"ÔÑÅ"; font-family: 'FontAwesome'; font-weight: normal;
  font-style: normal;}
.header-top .close {position:relative; text-indent:-9999em; width:17px; height:17px; background:url(../images/x-icon.png) no-repeat left top; float:right; background-size:17px 17px; display:block; right:17px;}



img {padding:0px; margin:0px; line-height:0px; outline:none; max-width:100%;}
.hp-banner-area {position:relative; margin:0 0 39px;}
