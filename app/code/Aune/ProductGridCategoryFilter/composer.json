{
    "name": "aune-io/magento2-product-grid-category-filter",
    "description": "Product grid category filter for Magento 2",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "7.0.2|7.0.4|~7.0.6|~7.1.0",
        "magento/framework": "101.0.*",
        "magento/module-catalog": "102.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2.0",
        "squizlabs/php_codesniffer": "3.2.2",
        "phpmd/phpmd": "@stable",
        "php-coveralls/php-coveralls": "~2.1.0"
    },
    "type": "magento2-module",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aune\\ProductGridCategoryFilter\\": ""
        }
    }
}
