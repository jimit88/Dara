<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                
                <resource id="Plumtree_Extensions::plumtreeextensions" title="Mail Catalog Subscriptions">
                        <resource id="Plumtree_RequestCatalog::requestcatalog" title="Request Catalog" sortOrder="30" />                            
                </resource>
                
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Plumtree_RequestCatalog::config_requestcatalog" title="Quick RFQ Management" />
                        </resource>
                    </resource>
                </resource>
                
            </resource>
        </resources>
    </acl>
</config>
